/* components/prolist/prolist.wxss */
@import '/miniprogram_npm/@vant/weapp/common/index.wxss';
.prolist {
  width: 96%;
  margin: 0px 2%;
  overflow: hidden;
}

.prolist .proitem {
  width: 49.5%;
  height: 250px;
  margin-top: 5px;
  float: left;
  background-color: #fff;
}
.prolist .prolist:nth-child(2n) {
  margin-left: 1%;
}

.prolist .proitem .itemimg {
  width: 100%;
  height: 176px;
  background-color: #ccc;
  overflow: hidden;
}
.prolist .proitem .itemimg .image {
  width: 100%;
}
